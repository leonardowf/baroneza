{
    "name": "baroneza",
    "version": "1.0.0",
    "description": "choo choo",
    "main": "index.js",
    "scripts": {
        "start": "node --inspect=5858 -r ts-node/register ./src/app.ts",
        "start:watch": "nodemon",
        "test": "jest",
        "coverage": "jest --coverage",
        "lint": "eslint . --ext .ts",
        "lint-and-fix": "eslint . --ext .ts --fix",
        "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write",
        "build": "tsc -p tsconfig.json",
        "pm2:delete-baroneza": "pm2 delete baroneza 2> /dev/null || true",
        "pm2:dashboard": "pm2 dashboard",
        "pm2:save": "pm2 save",
        "start:pm2": "rm -rf dist/ && npm run build && npm run pm2:delete-baroneza && pm2 start dist/src/app.js --name baroneza --force && npm run pm2:dashboard"
    },
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@octokit/types": "^5.2.0",
        "@types/express": "^4.17.4",
        "@types/jest": "^26.0.7",
        "@types/node": "^14.0.27",
        "@types/request": "^2.48.5",
        "@types/semver": "^7.1.0",
        "@types/swagger-ui-express": "^4.1.2",
        "@typescript-eslint/eslint-plugin": "^2.27.0",
        "@typescript-eslint/parser": "^2.27.0",
        "eslint": "^6.8.0",
        "eslint-config-prettier": "^6.10.1",
        "eslint-plugin-prettier": "^3.1.2",
        "jest": "^26.6.3",
        "nodemon": "^2.0.20",
        "prettier": "^2.0.4",
        "ts-jest": "^26.1.2",
        "ts-mockito": "^2.6.1",
        "ts-node": "^9.1.1",
        "tslint": "^6.1.1",
        "typescript": "^3.9.7"
    },
    "dependencies": {
        "@octokit/rest": "^18.0.3",
        "@slack/events-api": "^2.3.4",
        "@slack/web-api": "^5.15.0",
        "body-parser": "^1.19.0",
        "dotenv": "^8.2.0",
        "express": "^4.17.3",
        "jira-client": "^8.2.2",
        "pm2": "^5.2.2",
        "rxjs": "^6.5.5",
        "semver": "^7.2.2",
        "swagger-ui-express": "^4.6.0"
    },
    "nodemonConfig": {
        "ignore": [
            "**/*.test.ts",
            "**/*.spec.ts",
            ".git",
            "node_modules"
        ],
        "watch": [
            "src"
        ],
        "exec": "npm start",
        "ext": "ts"
    }
}
